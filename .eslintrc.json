{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "commonjs": true
    },
	"rules": {
		"no-console": 0,
		"semi": [2, "never"],
		"no-unexpected-multiline": 2,
		"import/no-extraneous-dependencies": "off",
		"import/no-unresolved": [
	      2, 
	      { "caseSensitive": false }
	    ],
		"indent": "off",
        "no-tabs": 0
	}
}